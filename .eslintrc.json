{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "prettier"
  ],
  "rules": {
    "prefer-arrow-callback": "error",
    "prefer-template": "error",
    "@typescript-eslint/no-empty-object-type": [
      "error",
      {
        "allowInterfaces": "with-single-extends"
      }
    ],
    "@typescript-eslint/no-unused-vars": "warn"
  }
}